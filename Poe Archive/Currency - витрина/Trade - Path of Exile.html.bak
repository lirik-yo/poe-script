<script>
document.addEventListener('DOMContentLoaded', function() {
  // Находим все элементы с классом filter-title
  const filterTitles = document.querySelectorAll('.filter-title');
  
  // Добавляем обработчик клика на каждый элемент
  filterTitles.forEach(title => {
    title.addEventListener('click', function() {
      // Получаем только текстовое содержимое (без тегов)
      const textContent = this.textContent || this.innerText;
      
      // Копируем в буфер обмена
      navigator.clipboard.writeText(textContent.trim())
        .then(() => {
          console.log('Текст скопирован: ', textContent.trim());
          // Можно добавить визуальное подтверждение (опционально)
          this.style.backgroundColor = '#e0ffe0';
          setTimeout(() => this.style.backgroundColor = '', 1000);
        })
        .catch(err => {
          console.error('Ошибка копирования: ', err);
        });
    });
  });
});
</script>



